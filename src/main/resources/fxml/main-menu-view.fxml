<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER"
      prefHeight="400.0"
      prefWidth="600.0"
      spacing="13"
      stylesheets="@../css/basicStyle.css,@../css/bg.css"
      xmlns="http://javafx.com/javafx/10.0.2-internal"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="jsmahy.ups_client.controller.MainMenuController">
    <Button fx:id="playButton" mnemonicParsing="false" onAction="#showPlayScene" styleClass="button" text="Play"/>
    <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logout" styleClass="button" text="Logout"/>
    <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exit" text="Exit"/>
</VBox>